<div class="form-horizontal">
    <div class="form-group">
        <div class="col-md-2">
            <h2>Enter Name</h2>
            <p>
                @Html.TextBox("txtSettingName")
            </p>
            <p>Error Name</p>
        </div>
        <div class="col-md-4">
            <h2>Enter Time Zone</h2>
            <p>@Html.TextBox("txtTimeZone")</p>
            <p>Error Time Zone</p>
        </div>
        <div class="col-md-4">
            <h2></h2>
            <p><input type="submit" name="btnSumbit" value="Submit" /></p>
            <p><input type="reset" name="btnCancel" value="Cancel" /></p>
        </div>
    </div>
</div>
